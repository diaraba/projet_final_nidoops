<% if user_signed_in? %>
    <%if current_user.try(:admin?) %>
        <%= render partial: 'accueils/admin' %>
    <%else%>
        <%= render partial: 'accueils/utilisateur' %>
    <%end%>    
<%end%>  

<% if structure_signed_in? %>
     <%= render partial: 'accueils/structure' %>
<%end%>  


<% if !structure_signed_in? && !user_signed_in? %>
    <%= render partial: 'accueils/autre' %>
<%end%>  