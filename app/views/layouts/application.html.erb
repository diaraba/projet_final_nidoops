<!DOCTYPE html>
<html>
  <head>
    <title>Nidoops</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_pack_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!-- Bootstrap 4 CSS and JavaScript -->
    <link href="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
    <script src="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"></script>
    <!-- jQuery JS -->
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <!-- Font Awesome 5 CSS -->
    <link href="https://use.fontawesome.com/releases/v5.0.6/css/all.css" rel="stylesheet">
    <!-- Style CSS -->
  </head>

  <body>
        <div class="page-wrapper chiller-theme toggled">
          <a id="show-sidebar" class="btn btn-sm btn-dark" href="#">
          <i class="fas fa-bars"></i>
          </a>
          <nav id="sidebar" class="sidebar-wrapper">
            <div class="sidebar-content">
              <div class="sidebar-brand">
                <a href="#">Nidoops</a>
                <div id="close-sidebar">
                    <i class="fas fa-times"></i>
                </div>
              </div>
              <div class="sidebar-header">
                <%if user_signed_in?%>
                  <% if current_user.profile_user.present? %>
                    <div class="user-pic">
                        <%= image_tag current_user.profile_user.image.variant(resize_to_limit: [100, 100]) if current_user.profile_user.image.attached? %>  
                    </div>
                  <% else %>
                    <div class="user-pic">
                        <img class="img-responsive img-rounded" src="data:image/png;base64,"
                          alt="User picture">
                    </div>
                  <% end %>

                <% elsif structure_signed_in? %>

                  <% if current_structure.profile_structure.present? %>
                    <div class="user-pic">
                        <%= image_tag current_structure.profile_structure.image.variant(resize_to_limit: [100, 100]) if current_structure.profile_structure.image.attached? %>  
                    </div>
                  <% else %>
                    <div class="user-pic">
                        <img class="img-responsive img-rounded" src="data:image/png;base64,"
                          alt="User picture">
                    </div>
                  <% end %>

                <% else %>
                  <div class="user-pic">
                      <img class="img-responsive img-rounded" src="data:image/png;base64,"
                        alt="User picture">
                  </div>
                <%end%>

                <div class="user-info">
                    <% if user_signed_in? %>
                      <span class="text-dark user-name ">
                      <strong><%=current_user.try(:name)   %></strong>
                      </span>  
                    <% else %>
                      <span class="user-name">
                      <strong><%=current_structure.try(:alias)   %></strong>
                      </span>                                          
                    <% end %>


                    <% if user_signed_in?%>
                      <% if current_user.try(:admin?) %>
                        <span class="user-role">Administrateur</span>
                      <% else %>
                        <span class="user-role">User</span>  
                      <% end %>
                    <% elsif structure_signed_in? %>  
                      <span class="user-role">Structure</span>        
                    <% else %>
                      <span class="user-role"></span>                     
                    <% end %>  

                    <span class="user-status">
                    <% if user_signed_in? || structure_signed_in? %>
                        <i class="fa fa-circle"></i>
                        <span>Online</span>
                        </span>
                    <% else %>
                        <i class="fa fa-circle" style="color:red"></i>
                        <span>Offline</span>
                        </span>                       
                    <% end %>  

                </div>
              </div>
              <!-- sidebar-header  -->

              <!-- sidebar-search  -->
              <div class="sidebar-menu">
                <ul>
                    <li class="header-menu">
                      <span  class="text-dark">General</span>
                    </li>
                      <% if user_signed_in? || structure_signed_in? %>
                          <% if  current_user.try(:admin?)%>
                          <li>
                              <a href="<%= dashboard_path %>">
                                <i class="fa fa-tachometer-alt text-white"></i>
                                <span >Dashboard</span>
                              </a>
                          </li>
                          <% else %>

                          <li>
                              <a href="<%= accueils_path() %>">
                                <i class="fa fa-tachometer-alt text-white"></i>
                                <span>Accueil</span>
                              </a>
                          </li>
                          <% end %>  
                          
                      <% else %>
                          <li>
                              <a href="<%= accueils_path() %>">
                                <i class="fa fa-tachometer-alt text-white"></i>
                                <span>Accueil</span>
                              </a>
                          </li>
                      <% end %>



                  <%if structure_signed_in? %>  
                          <li>
                              <a href="<%= annonces_path %>">
                                <i class="fa fa-tachometer-alt text-white"></i>
                                <span>Annonces</span>
                              </a>
                          </li>

                          <li>
                              <a href="<%= avis_offres_path() %>">
                                <i class="fa fa-tachometer-alt text-white"></i>
                                <span>Avis offres</span>
                              </a>
                          </li>
                          
                          <li>
                              <a href="<%= subscribers_structure_path(current_structure) %>">
                                <i class="fa fa-tachometer-alt text-white"></i>
                                <span>Abonnés</span>
                              </a>
                          </li>

                          <li>
                              <a href="<%= profile_structure_path(current_structure)  %>">
                                <i class="fa fa-tachometer-alt text-white"></i>
                                <span>Profile</span>
                              </a>
                          </li>

                    

                    <% elsif !structure_signed_in? && !user_signed_in? %>  

                          <li>
                              <a href="<%= a_la_une_for_others_path() %>">
                                <i class="fa fa-tachometer-alt text-white"></i>
                                <span>A la une</span>
                              </a>
                          </li>     

                          <li>
                              <a href="<%= structures_path() %>">
                                <i class="fa fa-tachometer-alt text-white"></i>
                                <span>Structures</span>
                              </a>
                          </li>  


                      

                    <% elsif user_signed_in? %>    
                      <% if  current_user.try(:admin?)%>

                          <li>
                              <a href="<%= users_path() %>">
                                <i class="fa fa-book text-white"></i>
                                <span>Liste des utilisateurs</span>
                              </a>
                          </li>   

                          <li>
                              <a href="<%= structures_path() %>">
                                <i class="fa fa-book text-white"></i>
                                <span>Liste des structures</span>
                              </a>
                          </li>     
                          
                          <li>
                              <a href="<%= new_activite_path %>">
                                <i class="fa fa-tachometer-alt text-white"></i>
                                <span>Ajouter activité</span>
                              </a>
                          </li>
                          
                          <li>
                              <a href="<%= profile_user_path(current_user) %>">
                                <i class="fa fa-tachometer-alt text-white"></i>
                                <span>Profile</span>
                              </a>
                          </li>
                          
                      <% else %>
                          <li>
                              <a href="<%= structures_user_path(current_user) %>">
                                <i class="fa fa-tachometer-alt text-white"></i>
                                <span>Structures</span>
                              </a>
                          </li>   

                          <li>
                              <a href="<%= a_la_une_for_user_user_path(current_user) %>">
                                <i class="fa fa-tachometer-alt text-white"></i>
                                <span>A la une</span>
                              </a>
                          </li>     

                          <li>
                              <a href="<%= profile_user_path(current_user) %>">
                                <i class="fa fa-tachometer-alt text-white"></i>
                                <span>Profile</span>
                              </a>
                          </li>  

                                    

                      <% end %>
                    <% end %>
                      

                    
                    <li class="header-menu header-menu-extra">
                      <span class="text-dark">Extra</span>
                        <% if user_signed_in? %>
                          <li>                             
                            <%= link_to destroy_user_session_path, method: :delete  do%>
                                <i class="fas fa-sign-out-alt text-white"></i>
                                <span class="text-danger">Déconnexion</span>
                            <% end %> 
                          </li>
                        <% elsif structure_signed_in? %>
                          <li>                             
                            <%= link_to destroy_structure_session_path, method: :delete  do%>
                                <i class="fas fa-sign-out-alt text-white"></i>
                                <span class="text-danger">Déconnexion </span>
                            <% end %> 
                          </li>
                        <% else %>
                          <li>                            
                            <%= link_to new_user_session_path  do%>
                                <i class="fas fa-sign-in-alt text-white"></i>
                                <span  class="text-primary">Se connecter</span>
                            <% end %>

                            <%= link_to "Admin invité", admin_guest_sign_in_path, method: :post , class:"mb-2 text-white btn btn-info", style:"width:50%; margin-left:18%;"%> 
                            <%= link_to "Utilisateur invité", guest_sign_in_path, method: :post , class:"mb-2 text-white btn btn-light", style:"width:55%; margin-left:18%;"%> 
                            <%= link_to "Structure invité", structure_sign_in_path, method: :post , class:"text-white btn btn-info", style:"width:55%; margin-left:18%;"%> 
                          </li>   
                        <%end%>

                                          
                    </li> 



                  </ul>
              </div>
                <!-- sidebar-menu  -->
              </div>
              <!-- sidebar-content  -->
              <div class="sidebar-footer">
                <a href="#">
                  <i class="fa fa-bell text-white"></i>
                  <span class="badge badge-pill badge-warning notification text-white">3</span>
                </a>
                <a href="#">
                  <i class="fa fa-envelope text-white"></i>
                  <span class="badge badge-pill badge-success notification">7</span>
                </a>
                <a href="#">
                  <i class="fa fa-cog text-white"></i>
                  <span class="badge-sonar "></span>
                </a>
                <a href="#">
                  <i class="fa fa-power-off text-white"></i>
                </a>
              </div>
            </nav>
            <!-- sidebar-wrapper  -->
            <main class="page-content">
              <div class="container-fluid">
                <p class="notice"><%= notice %></p>
                <p class="alert"><%= alert %></p>
                <%= yield %> 






                <hr  class="py-5">

                <footer class="text-center py-5">
                  <div class="mb-2">
                    <small>
                      © 2020 made with <i class="fa fa-heart" style="color:red"></i> by - <a target="_blank" rel="noopener noreferrer" href="https://azouaoui.netlify.com">
                        Mohamed Azouaoui
                      </a>
                    </small>
                  </div>
                  
                </footer>
              </div>
            </main>
            <!-- page-content" -->
          </div>
  </body>
</html>
